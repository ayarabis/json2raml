{
  "name": "json2raml",
  "displayName": "JSON to RAML",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayarabis/json2raml"
  },
  "description": "Convert JSON to RAML",
  "publisher": "CodeButter",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "JSON to RAML",
      "properties": {
        "json2raml.includeSourceAsExample": {
          "type": "boolean",
          "default": true,
          "description": "Whether to include the source JSON as an example in the RAML output"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
